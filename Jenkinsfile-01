pipeline{
    agent any
    stages{
        stage("Git-Checkout"){
            agent{
                label "master"
            }
            steps{
                git url: "https://github.com/SANATASMIA/hello-world-1/tree/master/webapp"
            }
        }  //stage-git-checkout  
        stage("Maven-Build"){
                agent{
                    label "linux-one"
            }
                steps{
                    sh "mvn clean install package"
            }
        } //stage-maven
    } //stages
} //pipeline
